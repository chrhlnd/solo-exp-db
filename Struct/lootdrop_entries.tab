tab lootdrop_entries myisam
	,(dbname eqemu)
	,(autoinc 10000)
	,(charset utf8)
	,(col lootdrop_id u32 notnull default 0)
	,(col item_id i32 notnull default 0)
	,(col item_charges u16 notnull default 1)
	,(col equip_item u32 notnull default 0)
	,(col chance f32 notnull default 1)
	,(col disabled_chance f32 notnull default 0)
	,(col minlevel bool notnull default 0)
	,(col maxlevel bool notnull default 127)
	,(col multiplier u32 notnull default 1)
	,(primary (asc lootdrop_id) (asc item_id))